// Subtracts the second number from the first
// - a: The number to subtract from
// - b: The number to subtract
// = The result of a - b, or zero if b > a
Nat/sub
: ∀(a: Nat)
  ∀(b: Nat)
  Nat
= λ{
  #zero: λb #zero{}
  #succ: λa.pred λ{
    #zero: #succ{a.pred}
    #succ: λb.pred (Nat/sub a.pred b.pred)
  }
}
